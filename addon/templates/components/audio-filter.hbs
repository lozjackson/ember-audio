<div>
  Filter: {{_frequency}} Hz
</div>
<div>
  {{input type="range" value=_frequency min=minValue max=maxValue step="0.0001"}}
</div>
<div>
  Q: {{_q}}
</div>
<div>
  {{input type="range" value=_q min="0" max="100" step="0.0001"}}
</div>
<div>
  Gain: {{_gain}}
</div>
<div>
  {{input type="range" value=_gain min="-18" max="18" step="0.001" }}
</div>
{{#if hasBlock}}
  {{yield processor}}
{{/if}}
